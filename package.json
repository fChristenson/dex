{
  "name": "dex",
  "version": "1.0.0",
  "description": "A slack bot for checking how happy your teams are",
  "main": "./app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fChristenson/dex.git"
  },
  "keywords": [
    "slack",
    "bot",
    "team",
    "happiness"
  ],
  "author": "Fredrik Christenson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fChristenson/dex/issues"
  },
  "homepage": "https://github.com/fChristenson/dex#readme",
  "scripts": {
    "start": "node server.js",
    "test": "gulp test",
    "pre-push": "ln -sf ../../pre-push.sh .git/hooks/pre-push",
    "pre-commit": "ln -sf ../../pre-commit.sh .git/hooks/pre-commit",
    "post-commit": "ln -sf ../../post-commit.sh .git/hooks/post-commit",
    "add-hooks": "npm run pre-commit && npm run pre-push && npm run post-commit",
    "create-db": "docker run -d --name db -p 7474:7474 neo4j/neo4j",
    "start-db": "docker start db",
    "setup-env": "npm run start-db",
    "make": "npm run add-hooks && npm run create-db"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-exit": "0.0.2",
    "gulp-istanbul": "^0.10.3",
    "gulp-jscs": "^3.0.2",
    "gulp-jshint": "^2.0.0",
    "gulp-mocha": "^2.2.0",
    "rewire": "^2.5.1",
    "sinon": "^1.17.2"
  },
  "dependencies": {
    "bluebird": "^3.1.1",
    "body-parser": "^1.14.2",
    "express": "^4.13.3",
    "ramda": "^0.19.1",
    "seraph": "^0.14.0"
  }
}
